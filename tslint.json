{
	"defaultSeverity": "warning",
	"extends": [
		"tslint:recommended"
	],
	"jsRules": {},
	"rules": {
		"comment-format": [
			true,
			"check-lowercase",
			"check-space",
			{
				"ignore-words": ["TODO"]
			}
		],
		"indent": [
			true,
			"tabs"
		],
		"interface-name": [
			true,
			"never-prefix"
		],
		"max-line-lenght": [
			true,
			500
		],
		"no-console": [
			false
		],
		"no-string-literal": false,
		"object-literal-sort-keys" : [
			false
		],
		"prefer-const": false,
		"radix": false,
		"trailing-comma": [
			false
		]
	},
	"rulesDirectory": []
}
